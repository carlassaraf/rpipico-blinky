{
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "build/asm-blink.elf",
            "name": "Debug with OpenOCD",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2040.cfg"
            ],
            "openOCDPreConfigLaunchCommands": [
                "set USE_CORE 0"
            ],
            "openOCDLaunchCommands": [
                "adapter speed 4000",
            ],
            "searchDir": [],
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "svdFile": "${env:PICO_SDK_PATH}/src/rp2040/hardware_regs/rp2040.svd",
            "preLaunchTask": "build",
            "postDebugTask": "clean"
        }
    ]
}